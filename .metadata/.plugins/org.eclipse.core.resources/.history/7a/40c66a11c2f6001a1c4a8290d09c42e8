import com.wellsfargo.fsd.jw.model.VisitorsModel;

public class VisitorService {
	
	public void  computeAge(VisitorsModel visitorModel) {
		
		Period p=Period.between(visitorModel.getDateOfBirth(), LocalDate.now());
		
	}

}
